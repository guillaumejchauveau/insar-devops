    "jasmine-core": "~3.9.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "~1.7.0",
    "karma-junit-reporter": "~2.0.1",
    "start-server-and-test": "^1.14.0",
    "tslint": "~6.1.3",
    "typescript": "~4.3.5"
